			category="webtest.dsl.ui.category"
			class="webtest.dsl.ui.WebTestDslExecutableExtensionFactory:org.eclipse.xtext.ui.wizard.template.TemplateNewProjectWizard"
			id="webtest.dsl.ui.wizard.WebTestDslNewProjectWizard"
			name="WebTestDsl Project"
			icon="icons/new_WebTestDsl_proj.gif"
